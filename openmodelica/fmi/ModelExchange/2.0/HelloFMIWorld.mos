loadString("
model HelloFMI20World
  Real x(start=1);
  parameter Real a=2;
equation
  der(x) = a * x;
end HelloFMI20World;
");
translateModelFMU(HelloFMI20World, version="2.0");
getErrorString();
importFMU("HelloFMI20World.fmu");
loadFile("HelloFMI20World_me_FMU.mo");
getErrorString();
simulate(HelloFMI20World_me_FMU);
val(x,0);
val(x,1);

