loadFile("./testDAEPackage.mo");
getErrorString();

simulate(testDAE.p5);
getErrorString();

val(x,0.0);
val(y,0.0);
val(z,0.0);

val(x,1.0);
val(y,1.0);
val(z,1.0);

setCommandLineOptions("+daeMode=dynamic");
simulate(testDAE.p5, simflags="-daeMode", method="ida");
getErrorString();

val(x,0.0);
val(y,0.0);
val(z,0.0);

val(x,1.0);
val(y,1.0);
val(z,1.0);

setCommandLineOptions("+daeMode=all");
simulate(testDAE.p5, simflags="-daeMode", method="ida");
getErrorString();

val(x,0.0);
val(y,0.0);
val(z,0.0);

val(x,1.0);
val(y,1.0);
val(z,1.0);



