//name: forceInlineComplexEq4
// status: correct

setCommandLineOptions("--postOptModules+=forceInlineFunctionInLoops --initOptModules+=forceInlineFunctionInLoops +d=backenddaeinfo,dumpLoops ");
loadString("
model foo
  function f
     input Real x1;
     input Real x2;
     output Real y1;
     output Real y2;
    protected
     Real a = x1 - x2 + 10;
     Real b = 23*a; 
    algorithm
     y1 := b*x2 - a;
     y2 := 3*x2-5;
  end f;

  Real x1, x2, x3, x4;
equation
  (x1, x2) = f(3*x1 + cos(time), 3*x2 +1+x3-5*x4);
  (x4, x3) = f(sin(time), x1 + x3 - x2);
end foo;
");
getErrorString();

simulate(foo);
getErrorString();

val(x1,{0,1});
val(x2,{0,1});
val(x3,{0,1});
